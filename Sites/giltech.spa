        {
          "elementParent": "headWrapper",
          "innerText": "function rgbToHex(r, g, b) {function componentToHex(c) {var hex = c.toString(16);return hex.length == 1 ? \"0\" + hex : hex;};return \"#\" + componentToHex(r) + componentToHex(g) + componentToHex(b);};function hexToRgb(hex) {var result = /^#?([a-f\\d]{2})([a-f\\d]{2})([a-f\\d]{2})$/i.exec(hex);return result ? {r: parseInt(result[1], 16), g: parseInt(result[2], 16), b: parseInt(result[3], 16)}: null;};function updateRgbDivColor($divId) {var $colorRatio = .25;var $Color = getNumberFromDiv($divId);if ($Color > 255) {writeElement($divId) = 255;$Color = getNumberFromDiv($divId);};$Color2 = Math.round(($Color) * $colorRatio);switch ($divId) {case \"redInput\": document.getElementById($divId).style.backgroundColor = rgbToHex( $Color,$Color2,$Color2 );break;case \"greenInput\": document.getElementById($divId).style.backgroundColor = rgbToHex( $Color2,$Color,$Color2 );break;case \"blueInput\": document.getElementById($divId).style.backgroundColor = rgbToHex( $Color2,$Color2,$Color );break;};writeElement(\"htmlInput\",rgbToHex(getNumberFromDiv(\"redInput\"), getNumberFromDiv(\"greenInput\"),getNumberFromDiv(\"blueInput\")));document.getElementById(\"contentLabel\").style.backgroundColor = readElement(\"htmlInput\")};updateRgbDivColor('redInput');updateRgbDivColor('greenInput');updateRgbDivColor('blueInput');",
