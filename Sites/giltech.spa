          "onChange": "var $hex = hexToRgb(readElement(\"htmlInput\"));writeElement(\"redInput\",$hex.r);writeElement(\"greenInput\",$hex.g);writeElement(\"blueInput\",$hex.b);document.getElementById(\"contentLabel\").style.backgroundColor = readElement(\"htmlInput\");",
